{
  "include": [
    "src",
  ],
  "compilerOptions": {
    "lib": [
      "ES2022"
    ],
    "target": "es2022",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "jsx": "react",
    "noEmit": true,
    "strictNullChecks": true,
  }
}
